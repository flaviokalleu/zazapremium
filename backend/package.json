{
  "name": "zazap-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:pm2": "pm2 start ecosystem.config.cjs --env production",
    "restart:pm2": "pm2 restart zazap-backend || pm2 start ecosystem.config.cjs --env production",
    "stop:pm2": "pm2 delete zazap-backend || true",
    "dev": "nodemon --legacy-watch -e js --watch index.js --watch routes --watch controllers --watch services --watch models --watch middleware --watch config --ignore privated --ignore uploads --ignore **/*.json --ignore **/creds.json --ignore **/pre-key-*.json --ignore **/sender-key-*.json --ignore **/session-*.json --ignore **/app-state-sync-key-*.json --delay 2000 index.js",
    "dev:server": "ts-node-dev --respawn --transpile-only --poll --ignore-watch privated --ignore-watch uploads --ignore-watch dist --watch src src/server.ts",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "setup:saas": "node scripts/setup-saas.js",
    "migration:create": "npx sequelize-cli migration:generate --name",
    "model:create": "npx sequelize-cli model:generate --name",
    "seed:create": "npx sequelize-cli seed:generate --name"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@whiskeysockets/baileys": "^6.7.19",
    "audio-decode": "^2.2.3",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^4.18.2",
    "fca-unofficial": "^1.3.10",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.3",
    "glob": "^11.0.3",
    "instagram-private-api": "^1.46.1",
    "jimp": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "puppeteer": "^24.19.0",
    "qrcode": "^1.5.4",
    "redis": "^5.8.2",
    "sequelize": "^6.37.1",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^4.8.1",
    "web-push": "^3.5.0",
    "whatsapp-web.js": "^1.34.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  }
}
